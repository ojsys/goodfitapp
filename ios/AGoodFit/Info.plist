    <array>
      <dict>
        <key>CFBundleURLSchemes</key>
        <array>
          <string>com.goodfitapp.AGoodFit</string>
        </array>
      </dict>
      <dict>
        <key>CFBundleURLSchemes</key>
        <array>
          <string>com.googleusercontent.apps.485801760062-rkncj0j421iis1i3c96a538kj5cqrajm</string>
        </array>
      </dict>
    </array>
    <key>CFBundleVersion</key>
    <string>1</string>
    <key>GMSApiKey</key>
    <string>YOUR_IOS_GOOGLE_MAPS_API_KEY</string>
